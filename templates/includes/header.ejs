<header class="header">
  <h1>
    <a href="/" title="<%= themeConfig.siteName %>">
      <%= themeConfig.siteName %>
    </a>
  </h1>
  <nav>
    <h2>导航</h2>
    <% menus.forEach(function(menu) { %>
      <li>
        <% if (menu.openType === 'External') { %>
          <a
            href="<%= menu.link %>"
            title="<%= menu.name %>"
            target="_blank"
            >
            <%= menu.name %>
          </a>
        <% } else { %>
          <a
            href="<%= menu.link %>"
            title="<%= menu.name %>"
            class="<%= menu.activeClass %>"><%= menu.name %></a>
        <% } %>
      </li>
    <% }); %>
  </nav>
</header>